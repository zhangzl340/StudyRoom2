<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.studyroom.mapper.CarouselMapper">

    <select id="selectActiveCarousels" resultType="com.studyroom.entity.Carousel">
        SELECT * FROM carousels WHERE status = 'active' ORDER BY id ASC
